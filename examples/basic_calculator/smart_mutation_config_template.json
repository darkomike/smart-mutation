{
  "inputPaths": [
    "lib/"
  ],
  "outputDir": "output",
  "mutationTypes": [
    "arithmetic",
    "logical",
    "relational",
    "conditional",
    "increment",
    "assignment"
  ],
  "runTests": true,
  "testCommand": "dart test example/test/test.dart",
  "enableTracking": true,
  "useCumulative": true,
  "verbose": false,
  "excludePatterns": [
    "**/generated/**",
    "**/*.g.dart"
  ],
  "includePatterns": [
    "**/*.dart"
  ],
  "lineRanges": {
    "lib/calculate.dart": {
      "startLine": 1,
      "endLine": 500
    }
  },
  "parallel": true,
  "maxThreads": 2,
  "mutationEngine": "rule",
  "llmConfig": {
    "enabled": false,
    "provider": "gemini",
    "model": "1.5-flash-latest",
    "apiKey": "YOUR_API_KEY_HERE",
    "timeout": 30,
    "temperature": 0.7,
    "maxTokens": 4000
  }
}
